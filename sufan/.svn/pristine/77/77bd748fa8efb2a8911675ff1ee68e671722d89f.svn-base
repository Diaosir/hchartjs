input[type="date"]{
  width: 170px;
  font-size: 17px;
}
input[type="text"]{
  border: 2px solid #ecf0f1;
  border-bottom: 2px solid #ccc;
  text-align: center;
  height: 30px;
}
input[type='text']:focus{
  border-bottom: 2px solid #2C3E50;
  -webkit-transition: all .5s;
  -moz-transition: all .5s;
  -ms-transition: all .5s;
  -o-transition: all .5s;
  transition: all .5s;

}
input[type="radio"]+ span,input[type="checkbox"]+ span:before{
  opacity: 0;
  filter: alpha(opacity=0);
  -webkit-transition: all .5s;
  -moz-transition: all .5s;
  -ms-transition: all .5s;
  -o-transition: all .5s;
  transition: all .5s;
}
input[type="radio"]:checked +span,input[type="checkbox"]:checked +span:before{
  opacity: 1;
  filter: alpha(opacity=100);
}
.radio-box {
  display: inline-block;
  width: 12px;
  height: 12px;
  position: relative;
  top:-2px;
  background: #ecf0f1;
  vertical-align: middle;
  border-radius: 100%;
  border: 2px solid #ccc;
}
.radio-box input {
  position: absolute;
  opacity: 0;
  filter: alpha(opacity=0);
  top:-3px;
  left: -4px;
  width: 100%;
  height: 100%;
  z-index: 100;
}
.radio-box span { 
  display: block;
  width: 8px;
  height: 8px;
  border-radius: 100%;
  position: absolute;
  top:2px;
  left: 2px;
  background: #2C3E50;
  z-index: 1;
}
.checkbox span:before{
  position: absolute;
  top: -2px;
  left: 1px;
}
.checkbox{
  display: inline-block;
  width: 14px;
  height: 14px;
  position: relative;
  top: -2px;
  background: #ecf0f1;
  vertical-align: middle;
  border:2px solid #ccc;
}
.checkbox input {
  position: absolute;
  opacity: 0;
  filter: alpha(opacity=0);
  top:-3px;
  left: -4px;
  width: 100%;
  height: 100%;
  z-index: 100;
}
