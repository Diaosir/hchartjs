/*3D变化*/
 .container-3d{
      left: 50%;
      position: absolute;
      z-index: 300;
      margin: 200px auto;
      -webkit-perspective:600;
      -webkit-perspective-origin:10% 10%;
    }
    .area-3d{
        -webkit-transform-style:preserve-3d;   
        -webkit-animation:myframe 5s linear infinite;
        -webkit-transform-origin:10%;
        position:relative;
    }
    .page{
      width: 200px;
      height: 200px;
      margin: 0 auto;
      opacity: .75;
      color: red;
      line-height: 200px;
      font-size: 100px;
      text-align: center;
      position: absolute;
      border-radius:10px; 

    }
    @-webkit-keyframes myframe{
      0%{
        -webkit-transform:rotateY(0deg);
      }
      100%{
        -webkit-transform:rotateY(360deg) ;
      }
    }
    /*笼罩层*/
    .model{
      width: 100%;
      height: 100%;
      background: rgba(0,0,0,.5);
      position: fixed;
      top:0;
      left: 0;
      z-index: 300;
      -webkit-transition: all 1s;
      -moz-transition: all 1s;
      -ms-transition: all 1s;
      -o-transition: all 1s;
      transition: all 1s;
    }
    
